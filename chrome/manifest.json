{
	"name": "Where is my tab",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Search for title and url in currently open tabs.",

	"icons": {
		"16" : "icon16.png",
		"48" : "icon16.png",
		"128" : "icon16.png"
	},

	"browser_action": {
		"default_icon" : "icon19.png",
		"default_title": "Where is my tab",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs"
	],
	
	"content_security_policy": "script-src 'self'; object-src 'self'"
}
